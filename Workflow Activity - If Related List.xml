<name>wf_activity_definition_e69260bf34c1d10095cf93f368f0abfb</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="wf_activity_definition"&gt;&lt;wf_activity_definition action="INSERT_OR_UPDATE"&gt;&lt;action&gt;false&lt;/action&gt;&lt;attributes/&gt;&lt;category&gt;Conditions&lt;/category&gt;&lt;default_height/&gt;&lt;default_width/&gt;&lt;description/&gt;&lt;image/&gt;&lt;js_class_name&gt;If_Related_List&lt;/js_class_name&gt;&lt;name&gt;If Related List&lt;/name&gt;&lt;script&gt;// Workflow activity definition handler class&amp;#13;
//&amp;#13;
// Implement activity definition handling in the onExecute method&amp;#13;
//&amp;#13;
// Implement any event handlers for the activity definition as a method named 'on[event_name]'&amp;#13;
//     For example, to handle the 'cancel' event, implement an 'onCancel' method&amp;#13;
//&amp;#13;
&amp;#13;
var If_Related_ListActivityHandler = Class.create();&amp;#13;
If_Related_ListActivityHandler.prototype = Object.extendsObject(WFActivityHandler, {&amp;#13;
&amp;#13;
   initialize: function() {&amp;#13;
      WFActivityHandler.prototype.initialize.call(this);&amp;#13;
   },&amp;#13;
&amp;#13;
   onExecute: function() {&amp;#13;
      // Define a GlideRecord for the related table&amp;#13;
      var gr = new GlideRecord(activity.vars.table);&amp;#13;
      // Check the related table field for the value on the current record in the&amp;#13;
      //  workflow table's field&amp;#13;
      gr.addQuery(activity.vars.rl_match,current.getValue(activity.vars.wf_match));&amp;#13;
      // Check our related table conditions as well in the query&amp;#13;
      gr.addEncodedQuery(activity.vars.condition);&amp;#13;
      gr.query();&amp;#13;
      if (gr.next())&amp;#13;
         activity.result = 'yes';&amp;#13;
      else&amp;#13;
         activity.result = 'no';&amp;#13;
   },&amp;#13;
&amp;#13;
   type: 'If_Related_ListActivityHandler'&amp;#13;
});&lt;/script&gt;&lt;style/&gt;&lt;sys_created_by&gt;Jason.Butz&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2013-11-21 16:07:56&lt;/sys_created_on&gt;&lt;sys_id&gt;e69260bf34c1d10095cf93f368f0abfb&lt;/sys_id&gt;&lt;sys_mod_count&gt;2&lt;/sys_mod_count&gt;&lt;sys_updated_by&gt;Jason.Butz&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2013-11-21 16:33:12&lt;/sys_updated_on&gt;&lt;table&gt;global&lt;/table&gt;&lt;/wf_activity_definition&gt;&lt;/record_update&gt;</payload>
